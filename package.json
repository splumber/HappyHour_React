{
  "name": "happyhour_react",
  "version": "1.0.0",
  "description": "Happy Hour App using Reactjs and MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splumber/HappyHour_React.git"
  },
  "author": "",
  "license": "ISC",
  "proxy": "http://localhost:3001/",
  "private": true,
  "bugs": {
    "url": "https://github.com/splumber/HappyHour_React/issues"
  },
  "homepage": "https://github.com/splumber/HappyHour_React#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.16.2",
    "express": "^4.16.3",
    "google-map-react": "^1.0.9",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.0.17",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-facebook-login": "^4.1.1",
    "react-geolocated": "^2.4.0",
    "react-geolocation": "^1.0.4",
    "react-google-maps": "^9.4.5",
    "react-live-clock": "^2.0.3",
    "react-moment": "^0.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
