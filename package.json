    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.16.2",
    "express": "^4.16.3",
    "google-map-react": "^1.0.9",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.0.17",
    "react-geolocated": "^2.4.0",
    "react-geolocation": "^1.0.4",
    "react-google-maps": "^9.4.5",
    "react-moment": "^0.8.1",
    "react-router-dom": "^4.3.1"
  }
